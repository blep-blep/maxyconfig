set $ms Mod1
font pango:noto-sans 9
exec_always --no-startup-id redshift -x
exec_always --no-startup-id redshift -O 5500
exec feh --bg-scale ~/Backgrounds/space.jpg
exec_always --no-startup killall back4.sh
exec_always --no-startup-id $HOME/back4.sh 0.010 ~/Backgrounds/gl.gif
exec --no-startup-id picom -b
exec --no-startup-id dex --autostart --environment i3
exec --no-startup-id xss-lock --transfer-sleep-lock -- i3lock --nofork
exec --no-startup-id nm-applet
floating_modifier $ms
bindsym $ms+Return exec alacritty
bindsym $ms+Shift+q kill
bindsym $ms+d exec --no-startup-id dmenu_run
bindsym $ms+Shift+Left move left
bindsym $ms+Shift+Down move down
bindsym $ms+Shift+Up move up
bindsym $ms+Shift+Right move right
bindsym $ms+Left split h
bindsym $ms+Right split v
bindsym $ms+f fullscreen toggle
bindsym $ms+Up layout stacking
bindsym $ms+Down layout tabbed
bindsym $ms+e layout toggle split
bindsym $ms+Shift+space floating toggle
bindsym $ms+space focus mode_toggle
bindsym $ms+a focus parent
set $ws1 "1"
set $ws2 "2"
set $ws3 "3"
set $ws4 "4"
set $ws5 "5"
set $ws6 "6"
set $ws7 "7"
bindsym $ms+1 workspace number $ws1
bindsym $ms+2 workspace number $ws2
bindsym $ms+3 workspace number $ws3
bindsym $ms+4 workspace number $ws4
bindsym $ms+5 workspace number $ws5
bindsym $ms+6 workspace number $ws6
bindsym $ms+7 workspace number $ws7
bindsym $ms+Shift+1 move container to workspace number $ws1
bindsym $ms+Shift+2 move container to workspace number $ws2
bindsym $ms+Shift+3 move container to workspace number $ws3
bindsym $ms+Shift+4 move container to workspace number $ws4
bindsym $ms+Shift+5 move container to workspace number $ws5
bindsym $ms+Shift+6 move container to workspace number $ws6
bindsym $ms+Shift+7 move container to workspace number $ws7
bindsym $ms+Shift+c reload
bindsym $ms+Shift+r restart
bindsym $ms+Shift+e exec poweroff 
bindsym $ms+shift+9 exec setxkbmap dk
bindsym $ms+shift+0 exec setxkbmap us
bindsym $ms+Tab exec waterfox-g4
bar {
i3bar_command i3bar -t
tray_output primary
position top
strip_workspace_numbers no
colors {
	separator #00000000
	background #0000fe0f
	statusline #00000000
	focused_workspace #0000ff0f  #282a36CC #FFFFFF
	}
}
